{
  "name": "@dino-dev/vite-plugin-use-sound",
  "version": "0.9.8",
  "description": "A vite plugin for playing sound effects",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "license": "MIT",
  "author": "Cody Lu",
  "files": [
    "dist",
    "src",
    "client.d.ts"
  ],
  "keywords": [
    "vite",
    "vite-plugin",
    "sound",
    "audio",
    "useSound",
    "dino-dev"
  ],
  "scripts": {
    "dev": "pnpm unbuild --stub",
    "build": "pnpm unbuild",
    "prepublishOnly": "npm run build",
    "prepack": "pnpm unbuild"
  },
  "peerDependencies": {
    "vite": ">=4.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/etag": "^1.8.1",
    "@types/node": "^14.17.3",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.3.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vite": "^5.0.12"
  },
  "lint-staged": {
    "*.{ts}": "eslint --fix --ext",
    "*.{ts,md}": "prettier --write"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "etag": "^1.8.1",
    "fast-glob": "^3.3.2"
  }
}
